{
  "ci": {
    "collect": {
      "staticDistDir": "dist",
      "numberOfRuns": 1,
      "url": ["http://127.0.0.1:4173/"]
    },
    "assert": {
      "assertions": {
        "categories:performance": ["warn", { "minScore": 0.6 }],
        "categories:accessibility": ["warn", { "minScore": 0.85 }],
        "categories:seo": ["warn", { "minScore": 0.8 }]
      }
    },
    "upload": {
      "target": "temporary-public-storage"
    }
  }
}
